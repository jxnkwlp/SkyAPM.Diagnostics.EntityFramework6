<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net461</TargetFramework>
		<RootNamespace>SkyAPM.Diagnostics.EntityFramework</RootNamespace>
		<PackageId>SkyAPM.Diagnostics.EntityFramework</PackageId>
		<PackageTags>SkyWalking;APM;Diagnostics;SqlClient</PackageTags>
		<Version>0.2.0</Version>
		<Authors>Passingwind</Authors>
		<Description>SkyAPM diagnostics for EntityFramework6</Description>
		<PackageProjectUrl>https://github.com/jxnkwlp/SkyAPM.Diagnostics.EntityFramework6</PackageProjectUrl>
		<RepositoryUrl>https://github.com/jxnkwlp/SkyAPM.Diagnostics.EntityFramework6</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="SkyAPM.Utilities.DependencyInjection" Version="1.3.0" />
		<PackageReference Include="EntityFramework" Version="6.1.0" />
	</ItemGroup>

	<ItemGroup>
		<Content Include="content/*.*" Pack="true" PackagePath="content/net461">
			<PackageCopyToOutput>true</PackageCopyToOutput>
		</Content>
	</ItemGroup>

</Project>
